	end
    if message == '.help' then
        chat('.help - Ğ¼ĞµĞ½Ñ | .withdraw - Ğ²Ñ‹Ğ²ĞµÑÑ‚Ğ¸ | .play (ĞºĞ¾Ğ»Ğ¸Ñ‡ĞµÑÑ‚Ğ²Ğ¾) - Ğ¸Ğ³Ñ€Ğ°Ñ‚ÑŒ | .balance - Ğ±Ğ°Ğ»Ğ°Ğ½Ñ || Ğ¡ĞºĞ¾Ğ»ÑŒĞºĞ¾ Ğ·Ğ°Ğ´Ğ¾Ğ½Ğ°Ñ‚Ğ¸Ğ» ÑÑ‚Ğ¾Ğ»ÑŒĞºĞ¾ Ğ¿Ğ¾ÑĞ²Ğ¸Ñ‚ÑŒÑÑ Ğ½Ğ° Ğ±Ğ°Ğ»Ğ°Ğ½ÑĞµ!')
