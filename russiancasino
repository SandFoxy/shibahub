			chat('ğŸ’¸ Ğ’Ğ°Ñˆ Ğ±Ğ°Ğ»Ğ°Ğ½Ñ Ğ½Ğ¸Ğ¶Ğµ, Ñ‡ĞµĞ¼ ÑƒĞºĞ°Ğ·Ğ°Ğ½Ğ½Ğ¾Ğµ Ğ²Ğ°Ğ¼Ğ¸ Ğ·Ğ½Ğ°Ñ‡ĞµĞ½Ğ¸Ğµ! ğŸ’¸')
			return
		end
		local num
		local num = math.random() * 2 - 1
		local oldBalance = plrChatted:GetAttribute('Donated')
		local newBalance = math.floor(num * slotValx + oldBalance)
               if newBalance < 0 then
                  newBalance = 0
                end
		if newBalance > oldBalance then
			chat('ğŸ’¸ Ğ¢Ñ‹ Ğ²Ñ‹Ğ¸Ğ³Ñ€Ğ°Ğ»! Ğ¢Ğ²Ğ¾Ğ¹ Ğ±Ğ°Ğ»Ğ°Ğ½Ñ: ' .. newBalance .. ' ğŸ’¸')
		elseif newBalance < oldBalance then
			chat('ğŸ’¸ Ğ¢Ñ‹ Ğ¿Ñ€Ğ¾Ğ¸Ğ³Ñ€Ğ°Ğ´! Ğ¢Ğ²Ğ¾Ğ¹ Ğ±Ğ°Ğ»Ğ°Ğ½Ñ: ' .. newBalance .. ' ğŸ’¸')
		else
			chat('ğŸ’¸ Ğ¢Ñ‹ Ğ½Ğ¸Ñ‡ĞµĞ³Ğ¾ Ğ½Ğµ Ğ²Ñ‹Ğ¸Ğ³Ñ€Ğ°Ğ»! Ğ‘Ğ°Ğ»Ğ°Ğ½Ñ Ğ¿Ñ€ĞµĞ¶Ğ½Ğ¸Ğ¹ ğŸ’¸')
		end
